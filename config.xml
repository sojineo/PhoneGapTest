<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.Neo.Test" versionCode="11" version="1.0.1">

	<!-- versionCode is optional and Android only -->
	<!--	<preference name="phonegap-version" value="2.9.0" />-->

	<gap:plugin name="org.apache.cordova.device" />
	<gap:plugin name="com.phonegap.plugins.pushplugin" />
	<plugin name="NetworkStatus" value="CDVConnection" />
	<gap:plugin name="org.apache.cordova.splashscreen" />
	<gap:plugin name="org.apache.cordova.network-information" />
	<gap:plugin name="org.apache.cordova.inappbrowser" />
	<gap:plugin name="org.apache.cordova.dialogs" />


	<preference name="orientation" value="portrait" />
	<preference name="fullscreen" value="false" />
	<preference name="target-device" value="universal" />
	<preference name="deployment-target" value="6.1" />
	<preference name="DisallowOverscroll" value="true" />





	<preference name="AutoHideSplashScreen" value="false" />
	<!-- Do not auto hide splash on Android -->
	<preference name="SplashScreenDelay" value="10000" />


	<name>Neo Test</name>
	<description>
		Neo Test
	</description>
	<author email="info@nlindia.com" href="http://nlindia.ae">
		Neologix Software Solutions
	</author>

	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>

	<gap:config-file platform="ios" parent="CFBundleShortVersionString">
		<string>1.0.0</string>
	</gap:config-file>

	<!-- Permissions -->
	<feature name="NetworkStatus">
		<param name="android-package" value="org.apache.cordova.NetworkManager" />
	</feature>

	<feature name="Network Status">
		<param name="blackberry-package" value="org.apache.cordova.network.Network" />
	</feature>

	<feature name="NetworkStatus">
		<param name="ios-package" value="CDVConnection" />
	</feature>

	<Capabilities>
		<Capability Name="ID_CAP_NETWORKING" />
	</Capabilities>

	<!-- SQL data connection -->
	<feature name="Storage">
		<param name="android-package" value="org.apache.cordova.Storage" />
	</feature>

	<feature name="App">
		<param name="android-package" value="org.apache.cordova.App" />
	</feature>

	<!-- iPhone and iPod touch -->
	<gap:splash src="assets/images/splash/IOS/320x480.PNG" gap:platform="ios" width="320" height="480" />
	<gap:splash src="assets/images/splash/IOS/640x960.PNG" gap:platform="ios" width="640" height="960" />

	<!-- iPhone 5 / iPod Touch (5th Generation) -->
	<gap:splash src="assets/images/splash/IOS/640x1136.PNG" gap:platform="ios" width="640" height="1136" />

	<!-- Splash screen Android-->
	<gap:splash src="assets/images/splash/Android/drawable-xhdpi/splash.png" />
	<gap:splash src="assets/images/splash/Android/drawable-ldpi/splash.png" gap:platform="android" gap:qualifier="ldpi" />
	<gap:splash src="assets/images/splash/Android/drawable-mdpi/splash.png" gap:platform="android" gap:qualifier="mdpi" />
	<gap:splash src="assets/images/splash/Android/drawable-hdpi/splash.png" gap:platform="android" gap:qualifier="hdpi" />
	<gap:splash src="assets/images/splash/Android/drawable-xhdpi/splash.png" gap:platform="android" gap:qualifier="xhdpi" />
	<gap:splash src="assets/images/splash/Android/drawable-xhdpi/splash.png" gap:platform="android" gap:qualifier="fr-xhdpi" />

	<!-- iPhone / iPod Touch  -->
	<icon src="assets/images/icons/iOS/icon-60.png" gap:platform="ios" width="60" height="60" />
	<icon src="assets/images/icons/iOS/icon-60@2x.png" gap:platform="ios" width="120" height="120" />

	<!-- iPad -->
	<icon src="assets/images/icons/iOS/icon-76.png" gap:platform="ios" width="76" height="76" />
	<icon src="assets/images/icons/iOS/icon-76@2x.png" gap:platform="ios" width="152" height="152" />
	<!-- Settings Icon -->
	<icon src="assets/images/icons/iOS/icon-small.png" gap:platform="ios" width="29" height="29" />
	<icon src="assets/images/icons/iOS/icon-small@2x.png" gap:platform="ios" width="58" height="58" />

	<!-- Spotlight Icon -->
	<icon src="assets/images/icons/iOS/icon-40.png" gap:platform="ios" width="40" height="40" />
	<icon src="assets/images/icons/iOS/icon-40@2x.png" gap:platform="ios" width="80" height="80" />

	<!-- iPhone / iPod Touch -->
	<icon src="assets/images/icons/iOS/icon.png" gap:platform="ios" width="57" height="57" />
	<icon src="assets/images/icons/iOS/icon@2x.png" gap:platform="ios" width="114" height="114" />

	<!-- iPad -->
	<icon src="assets/images/icons/iOS/icon-72.png" gap:platform="ios" width="72" height="72" />
	<icon src="assets/images/icons/iOS/icon-72@2x.png" gap:platform="ios" width="144" height="144" />
	<!-- iPhone Spotlight and Settings Icon -->
	<icon src="assets/images/icons/iOS/icon-small.png" gap:platform="ios" width="29" height="29" />
	<icon src="assets/images/icons/iOS/icon-small@2x.png" gap:platform="ios" width="58" height="58" />

	<!-- iPad Spotlight and Settings Icon -->
	<icon src="assets/images/icons/iOS/icon-50.png" gap:platform="ios" width="50" height="50" />
	<icon src="assets/images/icons/iOS/icon-50@2x.png" gap:platform="ios" width="100" height="100" />

	<!-- icon screen Android-->
	<icon src="assets/images/icon-60@2x.png" gap:platform="android" gap:qualifier="xhdpi" />

</widget>
